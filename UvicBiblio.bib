  annotation = {ADS Bibcode: 2015ApJ...799...54T},
  file = {/Users/joshuaissa/Zotero/storage/IULXZR3W/Travaglio et al. - 2015 - Testing the Role of SNe Ia for Galactic Chemical Evolution of p-nuclei with Two-dimensional Models a.pdf}
}

