@article{herwigGLOBALNONSPHERICALOSCILLATIONS2014,
  title = {{{GLOBAL NON-SPHERICAL OSCILLATIONS IN THREE-DIMENSIONAL}} 4{$\pi$} {{SIMULATIONS OF THE H-INGESTION FLASH}}},
  author = {Herwig, Falk and Woodward, Paul R. and Lin, Pei-Hung and Knox, Mike and Fryer, Chris},
  year = {2014},
