@article{xiongProduction$p$Nuclei2024,
  title = {Production of \$p\$ {{Nuclei}} from \$r\$-{{Process Seeds}}: {{The}} \${\textbackslash}ensuremath\{{\textbackslash}nu\}r\$ {{Process}}},
