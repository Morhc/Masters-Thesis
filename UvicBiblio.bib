  file = {/Users/joshuaissa/Zotero/storage/LFLT3UAJ/Fröhlich et al. - 2006 - Neutrino-Induced Nucleosynthesis of A64 Nuclei The νp Process.pdf}
}

@article{rappSensitivityPProcessNucleosynthesis2006,
  title = {Sensitivity of P-{{Process Nucleosynthesis}} to {{Nuclear Reaction Rates}} in a 25 {{Msolar Supernova Model}}},
